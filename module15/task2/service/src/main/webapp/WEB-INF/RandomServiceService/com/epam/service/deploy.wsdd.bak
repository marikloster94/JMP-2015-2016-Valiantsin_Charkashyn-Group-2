<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from RandomServiceService WSDL service -->

  <service name="RandomService" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://service.epam.com"/>
      <parameter name="wsdlServiceElement" value="RandomServiceService"/>
      <parameter name="schemaQualified" value="http://service.epam.com"/>
      <parameter name="wsdlServicePort" value="RandomService"/>
      <parameter name="className" value="com.epam.service.RandomServiceSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="RandomService"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="randomNumberGenerator" qname="operNS:randomNumberGenerator" xmlns:operNS="http://service.epam.com" returnQName="retNS:randomNumberGeneratorReturn" xmlns:retNS="http://service.epam.com" returnType="rtns:int" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:lowerBound" xmlns:pns="http://service.epam.com" type="tns:int" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:upperBound" xmlns:pns="http://service.epam.com" type="tns:int" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <parameter name="allowedMethods" value="randomNumberGenerator"/>

  </service>
</deployment>
